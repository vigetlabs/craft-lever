{
    "name": "viget/craft-lever",
    "description": "Wrapper to integrate with the Lever API",
    "type": "craft-plugin",
    "version": "3.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "lever"
    ],
    "support": {
        "docs": "https://github.com/vigetlabs/craft-lever/blob/master/README.md",
        "issues": "https://github.com/vigetlabs/craft-lever/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Trevor Davis",
            "homepage": "https://www.viget.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0",
        "php": "^8.0.2"
    },
    "autoload": {
        "psr-4": {
          "viget\\lever\\": "src/"
        }
    },
    "extra": {
        "name": "Lever",
        "handle": "lever",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/vigetlabs/craft-lever/master/CHANGELOG.md",
        "components": {
            "leverService": "viget\\lever\\services\\LeverService"
        },
        "class": "viget\\lever\\Lever"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
